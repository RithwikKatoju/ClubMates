@model ClubMates.Models.Department

@{
    ViewBag.Title = "AddDepartment";
}

<h2>AddDepartment</h2>
<form action="/Department/" method="post">
    <label>Department Id</label>
    <input type="number" name="DepartmentId" value="@Model.DepartmentId" />
    <br />
    <label>Department Name</label>
    <input type="text" name="DepartmentName" value="@Model.DepartmentName" />
    <br />
    <button type="submit">Save</button>
</form>

